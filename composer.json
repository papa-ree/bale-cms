{
    "name": "paparee/bale-cms",
    "description": "Bale CMS Core",
    "keywords": [
        "paparee",
        "laravel",
        "bale-cms"
    ],
    "homepage": "https://github.com/paparee/bale-cms",
    "license": "MIT",
    "authors": [
        {
            "name": "Papa Ree",
            "email": "ricky.romdhoni@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "awssat/laravel-visits": "*",
        "blade-ui-kit/blade-icons": "*",
        "gg-innovative/larafirebase": "*",
        "hexadog/laravel-themes-manager": "*",
        "hisorange/browser-detect": "*",
        "icehouse-ventures/laravel-chartjs": "*",
        "jantinnerezo/livewire-alert": "*",
        "josiasmontag/laravel-recaptchav3": "*",
        "kreait/laravel-firebase": "*",
        "laravel-lang/common": "*",
        "laravel/sanctum": "*",
        "laravel/tinker": "*",
        "livewire/volt": "*",
        "mallardduck/blade-lucide-icons": "*",
        "opcodesio/log-viewer": "*",
        "rtconner/laravel-tagging": "*",
        "spatie/laravel-activitylog": "*",
        "spatie/laravel-permission": "*",
        "wire-elements/modal": "*",
        "yadahan/laravel-authentication-log": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3||^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Paparee\\BaleCms\\": "src/",
            "Paparee\\BaleCms\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paparee\\BaleCms\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "hexadog/laravel-theme-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paparee\\BaleCms\\BaleCmsServiceProvider"
            ],
            "aliases": {
                "BaleCms": "Paparee\\BaleCms\\Facades\\BaleCms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
